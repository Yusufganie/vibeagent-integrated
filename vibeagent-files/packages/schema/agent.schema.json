{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "name",
    "description",
    "tools",
    "workflow"
  ],
  "properties": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "tools": {
      "type": "array"
    },
    "workflow": {
      "type": "array"
    },
    "channels": {
      "type": "array"
    }
  }
}